   "source": [
    "import astropy.constants as const\n",
    "c = const.c.cgs.value\n",
    "foo = open('SHARC-II_350um_filter.txt','w')\n",
    "data = open('interpolated_SHARC-II_350um_filter.csv','r').readlines()\n",
    "\n",
    "for d in data:\n",
    "    foo.write('%e \\t %f \\n' % (c/float(d.split()[0])/10, float(d.split()[1])/100.))\n",
    "foo.close()"
   ]
